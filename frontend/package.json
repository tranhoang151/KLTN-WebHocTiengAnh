{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:5001",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-youtube": "^7.6.2",
    "chart.js": "^4.4.3",
    "firebase": "^12.3.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.2",
    "react-scripts": "5.0.1",
    "react-youtube": "^10.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env HTTPS=true SSL_CRT_FILE=./certs/localhost.pem SSL_KEY_FILE=./certs/localhost-key.pem react-scripts start",
    "dev": "cross-env FAST_REFRESH=true CHOKIDAR_USEPOLLING=true WATCHPACK_POLLING=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --passWithNoTests",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --passWithNoTests",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:integration:ci": "jest --config jest.integration.config.js --ci --coverage --maxWorkers=2",
    "test:all": "npm run test && npm run test:integration",
    "test:ci": "npm run test:coverage && npm run test:integration:ci",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,md}",
    "performance:test": "node ../scripts/performance-tests.js",
    "security:scan": "node ../scripts/security-scan.js",
    "coverage:report": "node ../scripts/coverage-report.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/dompurify": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "audit-ci": "^7.0.0",
    "cross-env": "^10.1.0",
    "dompurify": "^3.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "globals": "^16.4.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "puppeteer": "^22.0.0",
    "retire": "^5.0.0",
    "ts-jest": "^29.4.4"
  }
}